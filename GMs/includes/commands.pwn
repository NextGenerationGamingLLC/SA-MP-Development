CMD:cancelcall(playerid, params[])
{
    for(new i = 0; i < MAX_CALLS; i++)
	{
	    if(Calls[i][CallFrom] == playerid)
	    {
	        Calls[i][CallFrom] = INVALID_PLAYER_ID;
