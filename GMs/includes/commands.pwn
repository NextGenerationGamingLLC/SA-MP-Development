	for(new i = 0; i < sizeof(GateInfo); i++)
	{
		GetDynamicObjectPos(GateInfo[i][gGATE], X, Y, Z);
		if(IsPlayerInRangeOfPoint(playerid, GateInfo[i][gRange], X, Y, Z))
