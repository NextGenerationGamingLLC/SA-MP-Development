					return 1;
			    }
			}
			SendClientMessageEx(playerid, COLOR_GREY, "No one is near you to bite!");
		}
	}
	else
	{
	    if(EventKernel[EventType] == 4 && (GetPVarInt(playerid, "EventToken") == 1) && GetPVarType(playerid, "pEventZombie"))
		{
		    new Float:X, Float:Y, Float:Z;
