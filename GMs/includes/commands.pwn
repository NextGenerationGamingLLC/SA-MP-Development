	if(sscanf(params, "ds[30]", rank, rankname)) return SendClientMessageEx(playerid, COLOR_GREY, "USAGE: /adjustrankname [rank number 0-6] [rank name]");

	if(PlayerInfo[playerid][pRank] == 6)
	{
		if(rank < 0 || rank > 6)
		{
