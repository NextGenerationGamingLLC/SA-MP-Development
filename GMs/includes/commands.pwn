	if(!IsPlayerInAnyVehicle(playerid))
	{
		ApplyAnimation(playerid,"COP_AMBIENT","Coplook_watch", 4.0, 0, 0, 0, 0, 0, 1);
	}
    GameTextForPlayer(playerid, string, 5000, 1);
    return 1;
