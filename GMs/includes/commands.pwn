		}
		else
		{
	        SetPVarInt(playerid, "TackleMode", 0);
	        SendClientMessageEx(playerid, COLOR_LIGHTBLUE, "You've disabled tackling. You may now unholster your weapon. (/holster)");
			cmd_holster(playerid, params);
